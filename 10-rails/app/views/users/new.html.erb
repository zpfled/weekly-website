<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>
<div class="row">
	<div class="span6 offset3">
		<%= form_for(@user) do |f| %>
			<%= render 'shared/error_messages', object: f.object %>

			<%= render 'fields', f: f %>
		
			<%= f.submit "Create my account", class: "btn btn-large btn_primary" %>
		<% end %>
	</div>
</div>
